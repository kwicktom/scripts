{
    "@DEPENDENCIES" : [
        "../src/file1.js",
        "../src/file2.js"
    ],

    "@REPLACE" : {
        "a" : "b",
        "c" : "d"
    },
    
    "@DOC" : {
        "STARTDOC" : "/**[DOC_MARKUP]",
        "ENDDOC" : "[/DOC_MARKUP]**/",
        "OUTPUT" : "../doc/manual.md"
    },
    
    "@MINIFY" : {
        "@YUI" : [
            "--preserve-semi"
        ],
        
        "@CLOSURE" : [
            "--language_in=ECMASCRIPT5_STRICT"
        ],
        
        "@UGLIFY" : [
            "-m -c"
        ],
        
        "@CSSMIN" : [
            "--embed-images"
        ]
    },

    "@OUT" : "../build/package_output.min.js"
}